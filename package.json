{
    "name":  "@ai-sales/platform",
    "version":  "1.0.0",
    "description":  "Enterprise AI-powered sales platform for WhatsApp \u0026 Instagram",
    "type":  "module",
    "main":  "dist/production-index.js",
    "engines":  {
                    "node":  "\u003e=20.12.0",
                    "bun":  "\u003e=1.0.0"
                },
    "scripts":  {
                    "dev":  "cross-env NODE_ENV=development tsx src/production-index.ts",
                    "build":  "node scripts/build.mjs",
                    "build:production":  "tsc -p tsconfig.json",
                    "build:tsc":  "tsc -p tsconfig.json",
                    "prestart":  "node -e \"if(process.versions.bun) process.exit(1)\" || true",
                    "start":  "node dist/production-index.js",
                    "start:production":  "node dist/production-index.js",
                    "start:worker":  "node dist/worker-index.js",
                    "validate:production":  "node dist/startup/security-validations.js",
                    "check:production":  "node scripts/check-production.mjs",
                    "start:dev":  "cross-env NODE_ENV=development tsx src/production-index.ts",
                    "test":  "vitest run",
                    "test:coverage":  "vitest run --coverage",
                    "test:watch":  "vitest --watch",
                    "lint":  "eslint src/**/*.ts",
                    "lint:fix":  "eslint src/**/*.ts --fix",
                    "typecheck":  "tsc -p tsconfig.build.json --noEmit",
                    "db:migrate":  "tsx src/database/migrate.ts",
                    "db:migrate:js":  "node dist/database/migrate.js",
                    "docker:dev":  "docker-compose -f docker-compose.dev.yml up -d",
                    "docker:prod":  "docker-compose -f docker-compose.prod.yml up -d",
                    "docker:stop":  "docker-compose down",
                    "security:audit":  "npm audit",
                    "security:fix":  "npm audit fix --force",
                    "deps:update":  "npm update",
                    "deps:outdated":  "npm outdated",
                    "deps:clean":  "npm ls inflight \u0026\u0026 npm update \u0026\u0026 npm audit fix --force",
                    "logs":  "docker-compose logs -f api",
                    "check-env":  "node check-env.cjs",
                    "check-env:prod":  "NODE_ENV=production node check-env.cjs",
                    "ci:silencing":  "node scripts/ci/silencing-check.mjs",
                    "ci:hardening":  "npm run ci:silencing \u0026\u0026 npm run test:coverage"
                },
    "keywords":  [
                     "ai",
                     "sales",
                     "whatsapp",
                     "instagram",
                     "ecommerce",
                     "automation",
                     "iraq",
                     "enterprise"
                 ],
    "author":  "AI Sales Platform Team",
    "license":  "PROPRIETARY",
    "private":  true,
    "dependencies":  {
                         "@bull-board/api":  "^6.12.7",
                         "@bull-board/express":  "^6.12.7",
                         "@hono/node-server":  "^1.18.2",
                         "@hono/zod-validator":  "^0.7.2",
                         "@opentelemetry/api":  "^1.9.0",
                         "@opentelemetry/exporter-prometheus":  "^0.203.0",
                         "@opentelemetry/sdk-metrics":  "^2.0.1",
                         "@opentelemetry/sdk-trace-node":  "^2.0.1",
                         "@opentelemetry/semantic-conventions":  "^1.37.0",
                         "@sentry/node":  "^7.90.0",
                         "bcryptjs":  "^2.4.3",
                         "bullmq":  "4.18.3",
                         "cors":  "^2.8.5",
                         "crypto-js":  "^4.2.0",
                         "dotenv":  "^16.6.1",
                         "helmet":  "^7.1.0",
                         "hono":  "^4.0.0",
                         "ioredis":  "5.7.0",
                         "jsonwebtoken":  "^9.0.2",
                         "openai":  "^4.24.0",
                         "pg":  "^8.16.3",
                         "pino":  "^9.9.4",
                         "pino-std-serializers":  "^6.2.2",
                         "postgres":  "^3.4.0",
                         "prom-client":  "^15.1.3",
                         "rate-limiter-flexible":  "^4.0.0",
                         "redis":  "^5.8.1",
                         "uuid":  "^9.0.1",
                         "winston":  "^3.11.0",
                         "zod":  "^3.25.76"
                     },
    "devDependencies":  {
                            "@eslint/js":  "^9.34.0",
                            "@types/bcryptjs":  "^2.4.6",
                            "@types/cors":  "^2.8.17",
                            "@types/crypto-js":  "^4.2.1",
                            "@types/ioredis":  "^4.28.10",
                            "@types/jsonwebtoken":  "^9.0.5",
                            "@types/node":  "^20.19.11",
                            "@types/pg":  "^8.15.5",
                            "@types/uuid":  "^9.0.7",
                            "@typescript-eslint/eslint-plugin":  "^8.40.0",
                            "@typescript-eslint/parser":  "^8.40.0",
                            "@vitest/coverage-v8":  "^3.2.4",
                            "bun-types":  "^1.0.0",
                            "c8":  "^10.1.3",
                            "cross-env":  "^7.0.3",
                            "esbuild":  "^0.25.9",
                            "eslint":  "^9.34.0",
                            "pino-pretty":  "^11.3.0",
                            "ts-node":  "^10.9.2",
                            "tsx":  "^4.20.4",
                            "typescript":  "^5.5.4",
                            "typescript-eslint":  "^8.40.0",
                            "vitest":  "^3.2.4"
                        },
    "overrides":  {
                      "inflight":  "^1.0.6",
                      "glob":  "^10.0.0",
                      "node-domexception":  "^1.0.0"
                  }
}
