{
    "name":  "@ai-sales/platform",
    "version":  "1.0.0",
    "description":  "Enterprise AI-powered sales platform for WhatsApp \u0026 Instagram",
    "type":  "module",
    "main":  "dist/production-index.js",
    "engines":  {
                    "node":  "\u003e=20.12.0",
                    "bun":  "\u003e=1.0.0"
                },
    "scripts":  {
                    "dev":  "bun run --hot --env-file=.env.development src/index.ts",
                    "build":  "tsc -p tsconfig.json",
                    "build:production":  "tsc -p tsconfig.json",
                    "build:tsc":  "tsc -p tsconfig.json",
                    "prestart":  "node -e \"if(process.versions.bun) process.exit(1)\" || true",
                    "start":  "node dist/production-index.js",
                    "start:production":  "node dist/production-index.js",
                    "validate:production":  "node dist/startup/security-validations.js",
                    "start:dev":  "NODE_ENV=development bun run src/index.ts",
                    "test":  "vitest run",
                    "test:coverage":  "vitest run --coverage",
                    "test:watch":  "vitest --watch",
                    "test:instagram":  "node run-instagram-tests.js integration",
                    "test:instagram:unit":  "node run-instagram-tests.js unit",
                    "test:instagram:e2e":  "node run-instagram-tests.js e2e",
                    "test:instagram:performance":  "node run-instagram-tests.js performance",
                    "test:instagram:all":  "node run-instagram-tests.js all",
                    "test:instagram:stats":  "node run-instagram-tests.js integration stats",
                    "test:instagram:report":  "node run-instagram-tests.js integration report",
                    "test:all-new":  "node run-new-tests.cjs",
                    "test:comprehensive":  "bun run run-all-new-tests.ts",
                    "lint":  "eslint src/**/*.ts",
                    "lint:fix":  "eslint src/**/*.ts --fix",
                    "typecheck":  "tsc -p tsconfig.build.json --noEmit",
                    "test:all":  "bun run-all-tests.ts",
                    "test:all:list":  "bun run-all-tests.ts --list",
                    "test:security":  "bun run-all-tests.ts security",
                    "test:api":  "bun run-all-tests.ts api",
                    "test:repository":  "bun run-all-tests.ts repository",
                    "test:circuit":  "bun run-all-tests.ts circuit",
                    "test:migration":  "bun run-all-tests.ts migration",
                    "test:monitoring":  "bun run-all-tests.ts monitoring",
                    "test:critical":  "bun run-all-tests.ts security api repository",
                    "db:migrate":  "bun run src/database/migrate.ts",
                    "db:migrate:services":  "node run-service-control-migration.js",
                    "db:seed":  "bun run src/database/seed.ts",
                    "db:reset":  "bun run src/database/reset.ts",
                    "db:insert-test-merchant":  "node insert-test-merchant.js",
                    "docker:dev":  "docker-compose -f docker-compose.dev.yml up -d",
                    "docker:prod":  "docker-compose -f docker-compose.prod.yml up -d",
                    "docker:stop":  "docker-compose down",
                    "security:audit":  "bun audit",
                    "security:fix":  "npm audit fix --force",
                    "deps:update":  "npm update",
                    "deps:outdated":  "npm outdated",
                    "deps:clean":  "npm ls inflight \u0026\u0026 npm update \u0026\u0026 npm audit fix --force",
                    "logs":  "docker-compose logs -f api",
                    "check-env":  "node check-env.cjs",
                    "check-env:prod":  "NODE_ENV=production node check-env.cjs",
                    "db:seed:merchant-real":  "node scripts/seed-real-merchant.js",
                    "db:seed:merchant-services":  "node scripts/seed-merchant-services.js"
                },
    "keywords":  [
                     "ai",
                     "sales",
                     "whatsapp",
                     "instagram",
                     "ecommerce",
                     "automation",
                     "iraq",
                     "enterprise"
                 ],
    "author":  "AI Sales Platform Team",
    "license":  "PROPRIETARY",
    "private":  true,
    "dependencies":  {
                         "@hono/node-server":  "^1.18.2",
                         "@hono/zod-validator":  "^0.7.2",
                         "@opentelemetry/api":  "^1.9.0",
                         "@opentelemetry/sdk-metrics":  "^2.0.1",
                         "@opentelemetry/sdk-trace-node":  "^2.0.1",
                         "@sentry/node":  "^7.90.0",
                         "bcryptjs":  "^2.4.3",
                         "bullmq":  "4.18.3",
                         "cors":  "^2.8.5",
                         "crypto-js":  "^4.2.0",
                         "dotenv":  "^16.6.1",
                         "helmet":  "^7.1.0",
                         "hono":  "^4.0.0",
                         "ioredis":  "5.7.0",
                         "jsonwebtoken":  "^9.0.2",
                         "openai":  "^4.24.0",
                         "pg":  "^8.16.3",
                         "postgres":  "^3.4.0",
                         "prom-client":  "^15.1.3",
                         "rate-limiter-flexible":  "^4.0.0",
                         "redis":  "^5.8.1",
                         "uuid":  "^9.0.1",
                         "winston":  "^3.11.0",
                         "zod":  "^3.25.76"
                     },
    "devDependencies":  {
                            "@eslint/js":  "^9.34.0",
                            "@types/bcryptjs":  "^2.4.6",
                            "@types/cors":  "^2.8.17",
                            "@types/crypto-js":  "^4.2.1",
                            "@types/ioredis":  "^4.28.10",
                            "@types/jsonwebtoken":  "^9.0.5",
                            "@types/node":  "^20.19.11",
                            "@types/pg":  "^8.15.5",
                            "@types/uuid":  "^9.0.7",
                            "@typescript-eslint/eslint-plugin":  "^8.40.0",
                            "@typescript-eslint/parser":  "^8.40.0",
                            "@vitest/coverage-v8":  "^3.2.4",
                            "bun-types":  "^1.0.0",
                            "cross-env":  "^7.0.3",
                            "esbuild":  "^0.25.9",
                            "eslint":  "^9.34.0",
                            "tsx":  "^4.20.4",
                            "typescript":  "~5.9.2",
                            "typescript-eslint":  "^8.40.0",
                            "vitest":  "^3.2.4"
                        },
    "overrides":  {
                      "inflight":  "^1.0.6",
                      "glob":  "^10.0.0",
                      "node-domexception":  "^1.0.0"
                  }
}
